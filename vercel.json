{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "redirects": [
        {
            "source": "/:path*",
            "has": [
                {
                    "type": "host",
                    "value": "www.careermatch.fr"
                }
            ],
            "destination": "https://careermatch.fr/:path*",
            "permanent": true
        }
    ]
}